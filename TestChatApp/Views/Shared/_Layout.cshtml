<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - TestApp</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
<header>
    <nav class="navbar navbar-expand-md navbar-dark fixed-top bg-dark">
        <img src="~/Images/logo.png" width="49" height="47" alt="chatlogo"/>
        <div class="collapse navbar-collapse" id="navbarCollapse">
            <ul class="navbar-nav mr-auto offset-1 ">
                <li class="nav-item">
                    <a class="nav-link" href="#">Link1</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="#">Link2</a>
                </li>
                <li class="nav-item">
                    @Html.ActionLink("List of Users", "List", "Account", null, new {@class = "nav-link"})
                </li>
            </ul>
            <form class="form-inline mt-2 mt-md-0">
                @Html.Partial("_LoginPartial")
            </form>
        </div>
    </nav>
</header>
<br/>
<div class="container body-content">
    @RenderBody()
    <hr />
    <footer>
        <p class="text-center">&copy; @DateTime.Now.Year - TestApp</p>
    </footer>
</div>

@Scripts.Render("~/bundles/jquery")
@Scripts.Render("~/bundles/bootstrap")
@RenderSection("scripts", required: false)

<script src="~/Scripts/jquery-3.3.1.min.js" type="text/javascript"></script>
</body>
</html>
